{
	"manifest_version": 2,
	"name": "960 Grid System Overlay (Unofficial) ",
        "version": "2.0.2",
	"permissions": ["tabs", "http://*/", "https://*/"],
	"minimum_chrome_version": "5.0.308.0",
	"description": "Overlay \"960.gs\" guides over a webpage that uses the 960 Grid System.",
	"icons": {"128": "icons/logo-bw4_128.png", "48": "icons/logo-bw4_48.png", "16": "icons/logo-bw4_16.png" },
	"background" : {
		"scripts" : ["background.js"]
	},
	"options_page": "options.html",
	"page_action": {
		"default_icon": "icons/logo-bw4_16.png",
		"default_title": "960 Grid System"
	},
	"content_scripts" : [
		{
			"matches" : [
				"http://*/*",
				"https://*/*"
			],
			"js" : ["jquery-1.3.2.min.js", "contentscript.js"],
			"css" : ["overlay.css"],
			"all_frames" : false
		}
	]
}
